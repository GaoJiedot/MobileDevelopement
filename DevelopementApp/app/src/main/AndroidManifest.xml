        <activity android:name="com.example.Activity.Home"/>
        <activity android:name="com.example.Activity.CookBook"/>
        <activity android:name="com.example.Activity.ActionLibrary"/>
        <activity android:name="com.example.Activity.BodyData"/>
        <activity android:name="com.example.Activity.SportPlan"/>
